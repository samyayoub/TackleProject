<!DOCTYPE html>
<html>
  <head>
    <title>Tackleproject</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <link href="https://vjs.zencdn.net/5.12.6/video-js.css" rel="stylesheet">
    <script src="https://code.jquery.com/jquery-2.1.3.min.js"></script>
    <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/js/bootstrap.min.js"></script>

  </head>

  <body>

  <div class="navbar-wrapper">

    <nav class="navbar navbar-toggleable-md navbar-inverse bg-inverse dark">
      <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarTogglerDemo02" aria-controls="#navbarTogglerDemo02" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      
      <%= link_to 'Tackle Project', root_path, class: 'navbar-brand' %>
      <h6 class="header-nav-bar">A Project <br />Management Tool</h6>

      <div class="navbar-collapse collapse show" id="navbarTogglerDemo02" aria-expanded="true" style>
        <ul class="navbar-nav mr-auto mt-2 mt-md-0">
          <li class="nav-item">
            <%= link_to 'My Projects', '#', class: 'nav-link' %>
          </li>
        </ul>
          
          <ul class="navbar-nav">
            <li class="nav-item">
              <%= link_to 'Sign in', '#', class: 'nav-link' %>
            </li>
            <li class="nav-item">
              <%= link_to 'Sign up', '#', class: 'nav-link' %>
            </li>
          </ul>
              
  
          <form class="form-inline my-2 my-lg-0 search-box">
            <input class="form-control mr-sm-2" type="text" placeholder="Search">
            <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
          </form>

        </div>
    </nav>
  </div>







    <%= yield %>
    
  </body>
</html>
